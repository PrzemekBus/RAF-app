create table ras_device_images (
  di_id number generated by default as identity primary key,
  di_dev_id number not null, 
  di_url varchar2(255) not null, 
  di_upload_date date default sysdate,
  di_created      date not null,
  di_created_by   varchar2(255) not null,
  di_updated_by   varchar2(255),
  di_updated      date 
);

comment on table ras_device_images is 'table storing images of devices';
comment on column ras_device_images.di_id is 'unique identifier for the image';
comment on column ras_device_images.di_dev_id is 'identifier of the related device';
comment on column ras_device_images.di_url is 'url of the image';
comment on column ras_device_images.di_upload_date is 'upload date of the image';

create index idx_di_dev_id on ras_device_images(di_dev_id);